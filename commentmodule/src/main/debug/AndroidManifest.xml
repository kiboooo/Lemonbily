<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.commentmodule" >

    <application
        android:name=".debug.CommentApplication"
        android:allowBackup="true"
        android:label="@string/comment_app_name"
        android:supportsRtl="true"
        android:theme="@style/comment_AppTheme">
        <activity android:name=".view.CommentActivty">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>


</manifest>


